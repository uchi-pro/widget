{
  "name": "uchi-widget",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && mkdir ~/git/cdn/widget/$npm_package_version && cp dist/js/app*.js ~/git/cdn/widget/$npm_package_version/uchi-widget.js && cp dist/css/app*.css ~/git/cdn/widget/$npm_package_version/uchi-widget.css",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "autoprefixer": "^9.3.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "12.x",
    "eslint-plugin-compat": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-node": "7.x",
    "eslint-plugin-promise": "4.x",
    "eslint-plugin-standard": "4.x",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
